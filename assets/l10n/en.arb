{
    "@@locale": "en",
    "title": "Puzzle Hack",
    "@title": {
        "description": "Shown as the title of the puzzle"
    },
    "headerTitle": "Flutter Puzzle Hack",
    "@headerTitle": {
        "description": "Shown as the title in app"
    },
    "themeChangeTooltip": "Changing the theme will reset your score",
    "@themeChangeTooltip": {
        "description": "Shown as a tooltip for puzzle menu buttons"
    },
    "restartTooltip": "Restarting the puzzle will reset your score",
    "@restartTooltip": {
        "description": "Shown as a tooltip for the puzzle restart button"
    },
    "numberOfMoves": "Moves",
    "@numberOfMoves": {
        "description": "Indicates how many moves have been made on the current puzzle"
    },
    "numberOfTilesLeft": "Tiles",
    "@numberOfTilesLeft": {
        "description": "Indicates how many puzzle tiles are not in their correct position"
    },
    "startGame": "Start Game",
    "@startGame": {
        "description": "Shown before the puzzle is started"
    },
    "restart": "Restart",
    "@restart": {
        "description": "Shown after the puzzle is started and may be restarted"
    },
    "getReady": "Get ready...",
    "@getReady": {
        "description": "Shown when the puzzle is loading"
    },
    "waiting": "Waiting...",
    "@waiting": {
        "description": "Shown when the puzzle is waiting to exit"
    },
    "bye": "Bye...",
    "@bye": {
        "description": "Shown when the puzzle is finally in exit completed"
    },
    "next": "Next",
    "@next": {
        "description": "Shown as a button for next tip"
    },
    "view": "View",
    "@view": {
        "description": "Shown as a button for viewing attached tip"
    },
    "countdownGo": "GO!",
    "@countdownGo": {
        "description": "Shown when the countdown has just finished and the puzzle is about to start"
    },
    "tileLabelText": "Value: {value}, position: {x}, {y}",
    "@tileLabelText": {
        "description": "Semantic label for the puzzle tile",
        "placeholders": {
            "value": {
                "type": "String",
                "example": "5"
            },
            "x": {
                "type": "String",
                "example": "1"
            },
            "y": {
                "type": "String",
                "example": "0"
            }
        }
    },
    "numberOfMovesAndTilesLeftLabelText": "Moves: {numberOfMoves}, tiles: {tilesLeft}",
    "@numberOfMovesAndTilesLeftLabelText": {
        "description": "Semantic label for the puzzle tile",
        "placeholders": {
            "numberOfMoves": {
                "type": "String",
                "example": "30"
            },
            "tilesLeft": {
                "type": "String",
                "example": "10"
            }
        }
    },
    "durationLabelText": "{hours} hours {minutes} minutes {seconds} seconds",
    "@durationLabelText": {
        "description": "Semantic label for the Dashatar timer",
        "placeholders": {
            "hours": {
                "type": "String",
                "example": "2"
            },
            "minutes": {
                "type": "String",
                "example": "6"
            },
            "seconds": {
                "type": "String",
                "example": "24"
            }
        }
    },
    "halloweenSemanticsLabel": "Gia the Ghost"
}
